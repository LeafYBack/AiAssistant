{
    "manifest_version": 2,
    "name": "OneNote Assistant",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["script.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons": {
      "48": "icons/icon48.png"
    }
  }
  